   <children>
      <VBox fx:id="graph" fillWidth="false" prefHeight="200.0" prefWidth="20.0" style="-fx-background-color: red; -fx-border-color: white; -fx-border-width: 10;" HBox.hgrow="NEVER">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </VBox>
      <Text fx:id="text" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
   </children>
</HBox>
